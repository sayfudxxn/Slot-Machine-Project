<VBox prefHeight="400.0" prefWidth="600.0" fx:controller="slotMachineProject.SlotMachineController" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <MenuBar prefHeight="30.0">
        <menus>
          <Menu mnemonicParsing="false" text="New" />
          <Menu mnemonicParsing="false" text="Load">
            <items>
                  <MenuItem mnemonicParsing="false" text="Player A" />
              <MenuItem mnemonicParsing="false" text="Player B" />
                  <MenuItem mnemonicParsing="false" text="Player C" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="Save">
            <items>
              <MenuItem mnemonicParsing="false" text="Player A" />
                  <MenuItem mnemonicParsing="false" text="Player B" />
