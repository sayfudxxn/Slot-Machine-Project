      </FlowPane>
      <FlowPane alignment="CENTER" columnHalignment="CENTER" hgap="20.0" prefHeight="50.0" prefWidth="600.0">
         <children>
            <Label alignment="CENTER" text="Have a cheat Code? Enter here:" textAlignment="CENTER" />
            <TextField />
         </children>
      </FlowPane>
      <FlowPane alignment="CENTER" columnHalignment="CENTER" hgap="5.0" prefHeight="80.0" prefWidth="600.0">
         <children>
            <Label text="You Got" />
            <Label text="x You win Y, Congratulations" />
         </children>
      </FlowPane>
   </children>
