      <FlowPane alignment="CENTER" columnHalignment="CENTER" prefHeight="40.0" prefWidth="600.0">
         <children>
            <Button contentDisplay="CENTER" mnemonicParsing="false" prefWidth="100.0" text="Spin" textAlignment="CENTER" />
         </children>
      </FlowPane>
      <FlowPane alignment="CENTER" columnHalignment="CENTER" hgap="5.0" prefHeight="50.0" prefWidth="600.0">
